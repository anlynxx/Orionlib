loadstring(game:HttpGet("https://raw.githubusercontent.com/anlynxx/Orionlib/refs/heads/main/MY-HUBS/r6r15"))()
local Luminosity = loadstring(game:HttpGet("https://raw.githubusercontent.com/anlynxx/Orionlib/refs/heads/main/UIUX", true))()

local Window = Luminosity.new("Anlynx Hub V2", "v2.0.1", 4370345701)

local Tab1 = Window.Tab("Universal", 6026568198)
local Folder = Tab1.Folder("Player Settings", "Change Player Characteristics")
Folder.Slider("Speed", {Precise = true, Default = 16, Min = 1, Max = 425}, function(Status)
loadstring(game:HttpGet(('loadstring(game:HttpGet(('https://raw.githubusercontent.com/anlynxx/Orionlib/refs/heads/main/FullBright')))()
')))()

end)

Folder.Slider("Maximum Zoom Distance", {Precise = true, Default = 0, Min = 0, Max = 1000}, function(Status)
game:GetService("Players").LocalPlayer.CameraMaxZoomDistance = Status
end)

Folder.Button("FullBright", "Execute", function()
loadstring(game:HttpGet(('https://raw.githubusercontent.com/anlynxx/Orionlib/refs/heads/main/FullBright')))()
end)

